{
  "name": "svgsaver-reboot",
  "version": "0.10.0",
  "description": "[Fork] A modern, updated, typescript version of the popular Javascript library Hypercubed/svgsaver.",
  "main": "build/index.js",
  "typings": "build/index.d.js",
  "scripts": {
    "build": "tsc --project config/tsconfig.json && rollup --config config/rollup.config.js",
    "doc": "typedoc --options config/typedoc.html.js",
    "npm-publish": "pnpm build && pnpm doc && git add dist && git add docs && git commit -m 'new version' && pnpm publish"
  },
  "keywords": [
    "svgsaver",
    "reboot",
    "svg",
    "dom",
    "css",
    "png"
  ],
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@types/node": "^16.11.4",
    "rollup": "^2.58.3",
    "rollup-plugin-terser": "^7.0.2",
    "typedoc": "^0.22.7",
    "typescript": "^4.4.4"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/harshkhandeparkar/svgsaver-reboot.git"
  },
  "bugs": {
    "url": "https://github.com/harshkhandeparkar/svgsaver-reboot/issues"
  },
  "homepage": "https://github.com/harshkhandeparkar/svgsaver-reboot#readme"
}
